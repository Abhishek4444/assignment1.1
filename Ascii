
public class Ascii {
	 public static void main(String args[]){
	        int num[] = {65, 66};
	        String str =null;
	        for(int i: num){
	            str = Character.toString((char)i);
	            System.out.println(str);
	            System.out.println(  "-" +str);
	        }
	    }
}

